<?xml version='1.0' encoding='utf-8'?>
<widget id="com.toto" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Toppet</name>
    <description>
        Manage you Pet Life
    </description>
    <author email="eduu.porto@gmail.com" href="http://phonegap.com">
        DUTI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="localhost*" />
    <allow-navigation href="192.168*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <preference name="target-device" value="universal" />
    <preference name="orientation" value="default" />
    <preference name="permissions" value="none" />
</widget>
